<h1>食べ物一覧・追加</h1>
<p></p>
<!-- form_with部分 -->
<%= form_with model: Food.new,url: admin_food_index_path,method: :post do |f| %>
  <h4>ジャンル名</h4>
  <%= f.text_field :name %>
  <%= f.text_field :kcal %>kcal
  <!--テキストフィールドのところのnameはカラムのところと同じ記述にしなければならない-->
  <%= f.submit '新規登録' %>
<% end %>
<p></p>
<table>
 <tbody>
<p>ジャンル一覧</p>
<% @foods.each do |food| %>
  <tr>
  <td><%= food.name %></td>
  <td><%= food.kcal %></td>
  <td>
    <%#= link_to "Edit", edit_admin_genre_path(genre.id) %>
  </td>


  </tr>
<% end %>

</tbody>
</table>

