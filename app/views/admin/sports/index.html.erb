<h1>運動一覧・追加</h1>
<p></p>
<!-- form_with部分 -->
<%= form_with model: Sport.new,url: admin_sports_path,method: :post do |f| %>
  <h4>ジャンル名</h4>
  <%= f.text_field :name %>
  <%= f.text_field :kcal %>kcal
  <!--テキストフィールドのところのnameはカラムのところと同じ記述にしなければならない-->
  <%= f.submit '新規登録' %>
<% end %>
<p></p>
<table>
 <tbody>
<p>ジャンル一覧</p>
<% @sports.each do |sport| %>
  <tr>
  <td><%= sport.name %></td>
  <td><%= sport.kcal %></td>
  <td>
    <%#= link_to "Edit", edit_admin_genre_path(genre.id) %>
  </td>


  </tr>
<% end %>